{
  "name": "angular2-webpack-seed",
  "version": "0.0.0",
  "description": "Project seed for angular2 using Webpack as a module bundler",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --config ./tools/webpack.config.js ",
    "build": "npm run webpack --progress --colors --display-error-details",
    "dev-server": "webpack-dev-server --config ./tools/webpack.config.js --hot --progress --display-error-details",
    "start": "npm run dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexpods/angular2-webpack-seed"
  },
  "keywords": [
    "angular2",
    "webpack",
    "seed"
  ],
  "author": "alexpods <aleksey.podskrebyshev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexpods/angular2-webpack-seed/issues"
  },
  "homepage": "https://github.com/alexpods/angular2-webpack-seed",
  "dependencies": {
    "angular2": "^2.0.0-beta.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.0",
    "zone.js": "^0.5.10"
  },
  "devDependencies": {
    "ts-loader": "^0.7.2",
    "typescript": "^1.7.5",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
