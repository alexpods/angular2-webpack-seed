  },
  "homepage": "https://github.com/alexpods/angular2-webpack-seed",
  "dependencies": {
    "@reactivex/rxjs": "^5.0.0-alpha.7",
    "angular2": "^2.0.0-alpha.45",
    "reflect-metadata": "^0.1.2",
    "zone.js": "^0.5.8"
  },
  "devDependencies": {
    "ts-loader": "^0.6.0",
